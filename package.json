{
  "name": "@jensmeindertsma/fuel",
  "type": "module",
  "scripts": {
    "build": "esbuild ./src/server.ts --bundle --platform=node --outfile=./build/server.cjs",
    "clean": "rimraf ./build/",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint:check": "eslint src/server.ts",
    "lint:fix": "eslint . --fix",
    "start": "node ./build/server.cjs",
    "typecheck": "tsc"
  },
  "dependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@eslint/js": "^8.41.0",
    "@types/express": "^4.17.17",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "esbuild": "^0.17.19",
    "eslint": "^8.41.0",
    "prettier": "2.8.8",
    "rimraf": "^5.0.1",
    "typescript": "^5.0.4"
  }
}
